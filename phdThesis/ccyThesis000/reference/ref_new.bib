@article{2016CasagrandeHiptmairOstrowski,
    abstract = {We prove an a-priori error estimate for regularized
Curl-Curl Problems which are discretized by the
Interior Penalty/Nitsche's Method on meshes
non-conforming across interfaces. It is shown that
the total error can be bounded by the best
approximation error which in turn depends on the
concrete choice of the approximation space
Vh{\$}V{\_}{\{}h{\}}{\$}. In this work we show that
if Vh{\$}V{\_}{\{}h{\}}{\$}is the space of edge
functions of the first kind of order k we can expect
(suboptimal) convergence
O(hk−1){\$}O(h\^{}{\{}k-1{\}}){\$}as the mesh is
refined. The numerical experiments in (Casagrande et
al., SAM Report 2014-32, ETH Z{\"u}rich, 2014)
indicate that this bound is sharp for k=1{\$}k=1{\$}.
Moreover it is shown that the regularization term can
be made arbitrarily small without affecting the error
in the {$|$}⋅{$|$}curl{\$}{$\backslash$}lvert
{$\backslash$}cdot
{$\backslash$}rvert{\_}{\{}{$\backslash$}mathbf
{\{}curl{\}}{\}}{\$}semi-norm. A numerical experiment
shows that the regularization parameter can be chosen
in a wide range of values such that, at the same
time, the discrete problem remains solvable and the
error due to regularization is negligible compared to
the discretization error.},
    author = {Casagrande, R. and Hiptmair, R. and Ostrowski, J.},
    doi = {10.1186/s13362-016-0021-9},
    isbn = {2190-5983},
    journal = {J. Math. Ind.},
    number = {1},
    pages={1--25},
    title = {{An a priori error estimate for interior penalty
discretizations of the Curl-Curl operator on
non-conforming meshes}},
    volume = {6},
    year = {2016}
}

@inproceedings{2016CasagrandeWinkelmannHiptmairOstrowski,
    abstract = {We consider 3D Curl-Curl type of problems in the
presence of arbitrary, non-conforming
mesh-interfaces. The Interior Penalty/Nitsche's
Method (Stenberg, Computational mechanics, 1998) is
extended to these problems for edge functions of the
first kind. We present an a priori error estimate
which indicates that one order of convergence is lost
in comparison to conforming meshes due to
insufficient approximation properties of edge
functions. This estimate is sharp for first order
edge functions: In a numerical experiment the method
does not converge as the mesh is refined.},
