@Article{EfficientAssemblyOfHdivHcurl,
  author     = {Rognes, Marie E. and Kirby, Robert C. and Logg, Anders},
  journal    = {SIAM J. Sci. Comput.},
  title      = {Efficient Assembly of {H}(div) and {H}(curl) Conforming Finite Elements},
  year       = {2010},
  number     = {6},
  pages      = {4130-4151},
  volume     = {31},
  abstract   = {In this paper, we discuss how to efficiently evaluate and assemble general finite element variational forms on \$H(\mathrm{div})\$ and \$H(\mathrm{curl})\$. The proposed strategy relies on a decomposition of the element tensor into a precomputable reference tensor and a mesh-dependent geometry tensor. Two key points must then be considered: the appropriate mapping of basis functions from a reference element, and the orientation of geometrical entities. To address these issues, we extend here a previously presented representation theorem for affinely mapped elements to Piola-mapped elements. We also discuss a simple numbering strategy that removes the need to contend with directions of facet normals and tangents. The result is an automated, efficient, and easy-to-use implementation that allows a user to specify finite element variational forms on \$H(\mathrm{div})\$ and \$H(\mathrm{curl})\$ in close to mathematical notation.},
  bdsk-url-1 = {https://doi.org/10.1137/08073901X},
  doi        = {10.1137/08073901X},
  eprint     = {https://doi.org/10.1137/08073901X},
  url        = {https://doi.org/10.1137/08073901X},
}

